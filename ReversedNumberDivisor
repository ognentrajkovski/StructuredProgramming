#include <stdio.h>

int obratenbroj(int x){
    int obraten = 0, ostatok;
    for(;x>0; x/=10){
        ostatok = x%10;
        obraten = obraten*10 + ostatok;
    }
    return (obraten);
}

int brojnacifri(int k){
    int counter=0;
    while(k>0){
        k/=10;
        counter++;
    }
    return (counter);
}

int main(){
    int n;
    scanf("%d", &n);
    if(n<10)
        printf("Brojot ne e validen");
    else {
        for (int i = n-1; i > 0; i--) {
            if (obratenbroj(i) % brojnacifri(i) == 0) {
                printf("%d ", i);
                break;
            }

        }
    }
    return 0;
}
